def moverFicha(coord):
    # Comprobar el formato de la coordenada
    if ":" not in coord:
        print("[Error] Coordenada no es valida.")
        return
    # Separar las coordenadas
    origen, destino = separarCoordenada(coord)
    if comprobarCoordenada(origen) and comprobarCoordenada(destino):
        print("Son correctas")
    else:
        print("[Error] Coordenada no es valida.")
